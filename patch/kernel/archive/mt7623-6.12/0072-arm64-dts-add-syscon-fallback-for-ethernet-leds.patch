From: Frank Wunderlich <frank-w@public-files.de>
Date: Fri, 31 Jan 2025 18:31:18 +0100
Subject: [PATCH 72/93] arm64: dts: add syscon fallback for ethernet-leds

---
 arch/arm64/boot/dts/mediatek/mt7988a.dtsi | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm64/boot/dts/mediatek/mt7988a.dtsi b/arch/arm64/boot/dts/mediatek/mt7988a.dtsi
index 09febc065626..ec562514b468 100644
--- a/arch/arm64/boot/dts/mediatek/mt7988a.dtsi
+++ b/arch/arm64/boot/dts/mediatek/mt7988a.dtsi
@@ -297,7 +297,7 @@ apmixedsys: clock-controller@1001e000 {
 		};
 
 		pio: pinctrl@1001f000 {
-			compatible = "mediatek,mt7988-pinctrl";
+			compatible = "mediatek,mt7988-pinctrl", "syscon";
 			reg = <0 0x1001f000 0 0x1000>,
 			<0 0x11c10000 0 0x1000>,
 			<0 0x11d00000 0 0x1000>,
